@model Tour
<div class="col-md-3 col-sm-6 col-6 destinations-grids text-center">
	<div class="image-position position-relative">
		<img src="@Url.Content("~/Media/"+Model.AnhTour)" class="img-fluid" alt="">
		<div class="rating">
			<ul>
				<li><span class="fa fa-star"></span></li>
				<li><span class="fa fa-star"></span></li>
				<li><span class="fa fa-star"></span></li>
				<li><span class="fa fa-star"></span></li>
				<li><span class="fa fa-star"></span></li>
			</ul>
		</div>
	</div>
	<div class="destinations-info">
		<div class="caption mb-lg-3">
			<div class="package-info">
					<h6 class="mt-1"><span class="fa fa-map-marker mr-2"></span>@Model.TenTour</h6>
					<h5 class="my-2">$300</h5>
					<ul class="listing mt-3">
						<li><span class="fa fa-clock-o mr-2"></span>From: <span>@Model.NgayBd</span></li>
						<li><span class="fa fa-clock-o mr-2"></span>To: <span>@Model.NgayKt</span></li>

					</ul>
			 </div>
			<a href="booking.html">Book Now</a>
		</div>
	</div>
	<br>
</div>