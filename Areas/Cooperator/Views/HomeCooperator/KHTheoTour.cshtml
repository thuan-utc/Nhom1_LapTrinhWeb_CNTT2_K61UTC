@model IEnumerable<Nhom1_LapTrinhWeb_CNTT2_K61.Models.KhachHang>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "KHTheoTour";
    Layout = "~/Areas/Cooperator/Views/Shared/_LayoutCooperator.cshtml";
}



<div class="tab-content">
    <div class="tab-pane active">
        <div class="table-data">
            <div class="order">
                <div class="head">
                    <h3>Khách Hàng Theo Tour </h3>
                    <i class='bx bx-search'></i>
                    <i class='bx bx-filter'></i>

                </div>
                <table>
                    <thead>
                        <tr>
                            <th style="width:20%">
                                Name
                            </th>
                            <th>
                                People's Civilisation
                            </th>
                            <th>
                                Phone
                            </th>
                            <th>
                                Address
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TenKh)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.SoCmnd)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.Sdt)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DiaChi)
                                </td>
                            
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
